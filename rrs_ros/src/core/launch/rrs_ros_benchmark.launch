<?xml version="1.0"?>
 <launch>
        <!-- Unity2ROS modules -->
        <node pkg="rrs_ros" type="consul.sh" name="consul" output="screen" args="$(find rrs_ros)/scripts"/>
	<node pkg="rrs_ros" type="rrs_node_benchmark" name="rrs_ros" output="screen"/>
        <node pkg="rrs_ros" type="net2_node" name="net2_node" output="screen"/>
	
        <!-- Viz modules -->    
        <node name="rviz" pkg="rviz" type="rviz" args="-d $(find movo_viz)/rviz/robot_7dof_.rviz" output="screen"/>

</launch>
